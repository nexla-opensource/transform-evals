# API Keys for LLM Providers
# Copy this file to .env and fill in your actual API keys

# Anthropic API Key (required for judge model and Anthropic generator models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google API Key (required for Google generator models)
GOOGLE_API_KEY=your_google_api_key_here

# OpenAI API Key (required for OpenAI generator models)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: API keys for specific embedding/LLM services used in test cases
# These are only needed if you're running tests that require these services
# JINA_API_KEY=your_jina_api_key_here
# VOYAGE_API_KEY=your_voyage_api_key_here
# COHERE_API_KEY=your_cohere_api_key_here

